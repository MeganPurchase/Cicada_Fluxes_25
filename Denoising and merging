# DADA2 DENOISING AND MERGING PAIRED ENDS

qiime2 dada2 denoise-paired --i-demultiplexed-seqs trimmed-seqs.qza --p-trunc-len-f N --p-trunc-len-r N --p-trim-left-f 0
--p-trim-left-r 0 --o-representative-sequences rep-seqs-dada2.qza --o- table table-dada2.qza --o-denoising-stats stats-dada2.qza 
--verbose --p-n-threads 0

# REMOVE SINGLETONS

qiime feature-table filter-features --i-table table-dada2.qza --p-min-frequency 2 --o-filtered-table table-dada2-no-s.qza

